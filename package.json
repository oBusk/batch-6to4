{
  "name": "batch-6to4",
  "version": "0.0.1",
  "description": "A script to run 6to4 on any 6to4 IP in a batch of files.",
  "main": "script.js",
  "scripts": {
    "prep": "mkdirp in out",
    "prestart": "mkdirp in out && rimraf out/*",
    "start": "node script.js",
    "clean": "rimraf in/* out/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oBusk/batch-6to4.git"
  },
  "keywords": [
    "script",
    "6to4",
    "convert",
    "batch"
  ],
  "author": "Oscar Busk <oscar.busk@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/oBusk/batch-6to4/issues"
  },
  "homepage": "https://github.com/oBusk/batch-6to4#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
